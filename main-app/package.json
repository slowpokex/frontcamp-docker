    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "redux": "^3.7.2",
    "redux-logger": "^3.0.6",
    "redux-thunk": "^2.2.0",
    "rimraf": "^2.6.2",
    "webpack": "^3.11.0",
    "webpack-node-externals": "^1.6.0",
    "whatwg-fetch": "^2.0.3",
    "winston": "^2.4.0"
