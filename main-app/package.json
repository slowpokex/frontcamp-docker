{
  "name": "frontcamp_react_isomorphic_app",
  "version": "0.8.8",
  "description": "Frontcamp 3 React Isomorphic App",
  "main": "index.js",
  "author": "Mikita Isakau",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/slowpokex/frontcamp-docker.git"
  },
  "scripts": {
    "lint": "eslint ./src --fix",
    "test": "npm run test:unit && npm run test:int",
    "test:unit": "npm run mocha ./src/**/*.unit.js",
    "test:int": "npm run mocha ./src/**/*.int.js",
    "test:cov": "nyc npm run test:int",
    "mocha": "cross-env NODE_ENV=test mocha --exit --timeout 12000 --require babel-register --recursive",
    "clean": "rimraf dist",
    "prestart": "npm run clean && npm run -s build",
    "start:server": "node dist/server-bundle.js",
    "start": "npm run start:server",
    "build": "webpack",
    "build:watch": "webpack --watch",
    "window": "open http://localhost:8888",
    "run:dev:bundle": "wait-on ./dist/server-bundle.js && nodemon --exec babel-node ./dist/server-bundle.js && npm run window",
    "dev:server": "cross-env NODE_ENV=development DEBUG=frontcamp concurrently --kill-others \"npm run build:watch\" \"npm run run:dev:bundle\"",
    "dev": "npm run dev:server"
  },
  "dependencies": {
    "axios": "^0.18.0",
    "babel-polyfill": "^6.26.0",
    "babel-register": "^6.26.0",
    "babel-runtime": "^6.26.0",
    "bcrypt": "^1.0.3",
    "body-parser": "^1.18.2",
    "bootstrap": "^4.0.0",
    "compression": "^1.7.1",
    "connect-flash": "^0.1.1",
    "cookie-parser": "^1.4.3",
    "cors": "^2.8.4",
    "cross-env": "^5.1.3",
    "debug": "^3.1.0",
    "detect-node": "^2.0.3",
    "dotenv": "^5.0.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-config-prettier": "^2.9.0",
    "express": "^4.16.2",
    "express-session": "^1.15.6",
    "express-winston": "^2.4.0",
    "helmet": "^3.12.0",
    "html-webpack-plugin": "^2.30.1",
    "http-status": "^1.0.1",
    "jquery": "^3.3.1",
    "lodash": "^4.17.5",
    "mongoose": "^5.0.8",
    "mongoose-paginate": "^5.0.3",
    "morgan": "^1.9.0",
    "node-notifier": "^5.2.1",
    "passport": "^0.4.0",
